{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "sourceType": "module",
    "env": {
        "node": true
    },
    "rules": {
      "no-param-reassign": 0,
      "no-console": 0,
      "no-unused-vars": ["error", { "args": "none" }],
      "func-names": 0,
      "consistent-return": 0
    }
}
