{
    "extends": "airbnb-base",
    "plugins": [
        "import",
        "jest"
    ],
    "sourceType": "module",
    "env": {
        "node": true,
        "jest": true
    },
    "rules": {
      "no-param-reassign": 0,
      "no-console": 0,
      "no-unused-vars": ["error", { "args": "none" }],
      "no-eval": ["warn"],
      "func-names": 0,
      "consistent-return": 0,
      "jest/no-exclusive-tests": 2,
      "jest/no-identical-title": 2,
      "no-else-return": 0
    }
}
