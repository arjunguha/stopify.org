# Get all javascript files in the directory
# TODO(rachit): Implement this.
FILES = $(wildcard *.js)
COMPILED = $(FILES:%.js=cps/%-cps.js)

all: make-cps-dir $(COMPILED)

make-cps-dir:
	mkdir -p cps

cps/%-cps.js: %.js
	stopify -i $< -t cps > $@


# Rule to debug variables.
print-%  : ; @echo $* = $($*)
